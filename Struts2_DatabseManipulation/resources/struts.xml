<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="any" namespace="/" extends="struts-default">
		<action name="LoginActionUrl" class="online.solution.LoginAction">
			<result name="success">homePage.jsp</result>
			<result name="error">error.jsp</result>
		</action>
		
		<action name="insertActionUrl" class="online.solution.InsertAction">
			<result name="success">homePage.jsp</result>
			<result name="error">error.jsp</result>
		</action>
		
		<action name="showActionUrl" class="online.solution.ShowAction">
			<result name="success">show.jsp</result>
			<result name="error">error.jsp</result>
		</action>
		
	</package>
</struts>